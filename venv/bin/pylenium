#!/Users/joeldavis/Desktop/development/bottega/test-automation/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyleniumio==1.8.3','console_scripts','pylenium'
__requires__ = 'pyleniumio==1.8.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyleniumio==1.8.3', 'console_scripts', 'pylenium')()
    )
